<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_LLBlue">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_fragment_teamseason"
            android:textSize="@dimen/teamGuideSize"
            android:textColor="@color/color_grey"
            android:layout_marginTop="@dimen/content_horizontal_margin"
            android:layout_marginLeft="@dimen/content_horizontal_margin"/>

    </RelativeLayout>

    <ListView
        android:id="@+id/lv_teamSeason"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">



    </ListView>
</LinearLayout>